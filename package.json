{
  "name": "wishgrid",
  "private": true,
  "engines": {
    "node": "20.11.1",
    "pnpm": "8"
  },
  "pnpm": {
    "supportedArchitectures": {
      "os": [
        "linux",
        "current"
      ],
      "cpu": [
        "x64",
        "current"
      ]
    }
  },
  "scripts": {
    "postinstall": "prisma generate",
    "update-dependencies": "sh scripts/update-dependencies.sh",
    "env-pull": "vercel env pull .env",
    "dev": "next dev --turbo",
    "storybook": "storybook dev",
    "format": "prettier -w src",
    "check-format": "prettier --check src",
    "lint": "eslint --max-warnings 0 src",
    "check-types": "tsc",
    "check-dead-code": "knip",
    "quality": "pnpm run check-format && pnpm run lint && pnpm run check-types && pnpm run check-dead-code",
    "analyze-bundle": "cross-env ANALYZE=true next build",
    "migrate": "prisma migrate deploy",
    "build": "next build",
    "start": "next start"
  },
  "dependencies": {
    "@auth/prisma-adapter": "1.5.0",
    "@headlessui/react": "1.7.18",
    "@headlessui/tailwindcss": "0.2.0",
